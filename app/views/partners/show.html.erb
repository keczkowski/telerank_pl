<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-lg-10">
    <h2>Partner</h2>
    <ol class="breadcrumb">
      <li class="breadcrumb-item">
        <a href="<%= root_path %>">Strona Główna</a>
      </li>
      <li class="breadcrumb-item">
        <a href="<%= partners_path %>">Baza partnerów</a>
      </li>
      <li class="breadcrumb-item active">
        <strong>Partner</strong>
      </li>
    </ol>
  </div>
</div>

<div class="ibox">
  <div class="ibox-title">
    <h3>Partner</h3>
    <div class="ibox-tools">
      <%= link_to partners_path, class: "btn btn-sm btn-primary" do %>
      Wróć
      <% end %>
      <%= link_to edit_partner_path(@partner), class: "btn btn-sm btn-primary" do %>
      Edytuj
      <% end %>
    </div>
  </div>
  <div class="ibox-content">
    <p>
      <strong>Nazwa:</strong>
      <%= @partner.name %>
    </p>
  </div>
</div>

<%= content_for :searchbar do %>

<% end %>
